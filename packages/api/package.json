{
  "name": "@dealbot/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noemit",
    "lint": "eslint .",
    "update-types": "openapi-typescript https://raw.githubusercontent.com/IsThereAnyDeal/API/master/dist/openapi.json -o ./src/types.ts"
  },
  "exports": {
    "./*": {
      "default": "./dist/*.js",
      "types": "./src/*.ts"
    }
  },
  "dependencies": {
    "openapi-fetch": "^0.14.0"
  },
  "devDependencies": {
    "@dealbot/eslint-config": "workspace:*",
    "@dealbot/typescript-config": "workspace:*",
    "eslint": "^9.32.0",
    "openapi-typescript": "^7.8.0",
    "typescript": "^5.9.2"
  }
}
